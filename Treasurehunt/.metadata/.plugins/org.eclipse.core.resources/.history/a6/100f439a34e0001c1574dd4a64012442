package Treasurehunt;

import javax.swing.*;
import java.awt.*;


public class Plants {
	public Container Background(Container c) {
		JLabel zb = new JLabel();
		
		ImageIcon z = new ImageIcon("img\\3p.png");
		Image zz = z.getImage();
		Image zzz = zz.getScaledInstance(100, 100, Image.SCALE_SMOOTH);
		ImageIcon zzzz = new ImageIcon(zzz);
		zb = new JLabel(zzzz);
		zb.setBounds(500, 500, 100, 100);
		c.add(zb);
		
		return c;
	}

}
